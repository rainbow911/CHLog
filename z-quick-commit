
# 1、cd 到当前目录
currentDir=$(cd "$(dirname "$0")"; pwd)
cd ${currentDir}

# 2、提交操作
git add .
git commit -m "备份提交"
echo "\033[32m——-----commit finished—-----\033[0m"
git push -u origin master
echo "\033[32m——-----push finished—-----\033[0m"

echo "\033[32mgit 推送完毕🙃 ======================================================\033[0m"
echo "提交完成  🎉  🎉  🎉  🎈  🎈  🎈"

sleep 3
exit
